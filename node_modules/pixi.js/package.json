  },
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "del": "^2.2.0",
    "electron-prebuilt": "^1.3.2",
    "eslint": "^3.5.0",
    "floss": "^1.2.0",
    "gh-pages": "^0.11.0",
    "jaguarjs-jsdoc": "^1.0.1",
    "jsdoc": "^3.4.2",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "parallelshell": "^2.0.0",
    "pixify": "^1.5.0",
    "rimraf": "^2.5.3",
    "watch": "^0.19.1"
  },
  "browserify": {
    "transform": [
